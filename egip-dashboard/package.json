{
  "name": "@gostgroup/egip-client",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "author": "GOST Group",
  "main": "./dist/client/index.js",
  "scripts": {
    "dev": "npm i && cross-env NODE_ENV=development NODE_OPTIONS=--max-old-space-size=4096 webpack-dev-server",
    "dev:preprod": "npm i && cross-env NODE_ENV=development PLATFORM=preprod NODE_OPTIONS=--max-old-space-size=4096 webpack-dev-server",
    "dev:prod": "npm i && cross-env NODE_ENV=development PLATFORM=prod NODE_OPTIONS=--max-old-space-size=4096 webpack-dev-server",
    "devpoll": "cross-env NODE_ENV=development NODE_OPTIONS=--max-old-space-size=4096 webpack-dev-server --watch-poll 500 --watch-aggregate-timeout 300",
    "build": "npm i && cross-env NODE_ENV=production webpack",
    "serve": "http-server dist/client -s -p 3000 -P https://mmonitor.gost-group.com",
    "start": "npm run build && npm run serve",
    "test": "cross-env JEST_JUNIT_OUTPUT_DIR='./coverage' jest --coverage --colors",
    "test:watch": "cross-env JEST_JUNIT_OUTPUT_DIR='./coverage' jest --coverage --colors --colors --watch",
    "lint": "eslint --config ./.eslintrc.js src --ext .ts,.tsx",
    "lint:fix": "eslint --config ./.eslintrc.js src --ext .ts,.tsx --fix --quiet",
    "api:types": "swagger-codegen generate -i src/app/swagger.json -l typescript-angular -o src/app/swagger -Dmodels && npm run lint:fix"
  },
  "license": "MIT",
